{
  "name": "wwms",
  "version": "1.0.0",
  "description": "The \"_s\" for Timber: a dead-simple theme that you can build from. The primary purpose of this theme is to provide a file structure rather than a framework for markup or styles. Configure your Sass, scripts, and task runners however you would like!",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile:css": "postcss static/tailwind.css -o static/site.css",
    "watch:css": "postcss static/tailwind.css -o static/site.css -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timber/starter-theme.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timber/starter-theme/issues"
  },
  "homepage": "https://github.com/timber/starter-theme#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.4"
  }
}
